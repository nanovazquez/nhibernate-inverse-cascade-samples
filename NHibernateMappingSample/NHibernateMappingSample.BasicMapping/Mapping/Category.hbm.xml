<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="NHibernateMappingsSample.BasicMapping"
                   namespace="NHibernateMappingsSample.BasicMapping">

  <class name="Category">
    <id name="Id">
      <generator class="native" />
    </id>
    <property name="Name" />
    <bag name="Products">
      <key column="CategoryId" />
      <one-to-many class="Product" />
    </bag>
  </class>
</hibernate-mapping>